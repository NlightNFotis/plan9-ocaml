# -*- sh -*-
</$objtype/mkfile
<$TOP/mkconfig

# assumes the "includer" has defined TOP, SRC, INCLUDES, LIBS, SYSLIBS, and PROG

OBJS=${SRC:%.ml=%.cmo}

all:V: $PROG

$PROG: $OBJS
	$OCAMLC $INCLUDES $SYSLIBS $LIBS $prereq -o $target

clean:V:
	rm -f *.cm[ioxa] *.cmxa  *.o *.a  *.annot *.cmt*
	rm -f *~ .*~ *.exe gmon.out

<$TOP/mkcommon
<.depend
